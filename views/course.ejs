<%- include('partials/header') %>
<div class="container mt-5">
  <h1>Courses</h1>
  <div class="row">
    <div class="col-sm-8">
      <div class="card">
        <div class="card-body">

          <!-- Makes POST request to /register route -->
          <form action="/course" method="POST">
            <div class="form-group">
              <label for="c_id">Course Id</label>
              <input type="text" class="form-control" name="c_id" required>
            </div>
            <div class="form-group">
              <label for="c_name">Course Name</label>
              <input type="text" class="form-control" name="c_name" required>
            </div>
            <div class="form-group">
              <label for="sem">Semester</label>
              <input type="number" class="form-control" name="sem" required>
            </div>
            <div class="form-group">
              <label for="e_id">Elective Id</label>
              <input type="text" class="form-control" name="e_id" required>
            </div>
            <div class="form-group">
              <label for="dept_id">Deptartment Id</label>
              <input type="text" class="form-control" name="dept_id" required>
            </div>
            <div class="form-group">
              <label for="cc">Course Credit</label>
              <input type="number" class="form-control" name="cc" required>
            </div>
            <button type="submit" class="btn btn-dark m-2 p-2">Submit</button>
          </form>

        </div>
      </div>
    </div>


  </div>
</div>
<form action="/logout" method="POST">
  <div class="text-center">
    <button type="submit" class="btn-outline-danger m-2 p-2 btn-lg w-50">Logout</button>
  </div>
</form>

<%- include('partials/header') %>
